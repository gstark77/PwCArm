{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {

    "objBaseData": { "type": "object" },
    "objNetworkData": { "type": "object" }

  },
  "variables": {

  },

  "resources": [
    {
      "name": "[concat('vnet-',parameters('objBaseData').strAppName,parameters('objBaseData').strAppEnv)]",
      "type": "Microsoft.Network/virtualNetworks",
      "apiVersion": "[providers('Microsoft.Network','virtualNetworks').apiVersions[0]]",
      "location": "[resourceGroup().location]",
      "dependsOn": [
      ],
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[concat(parameters('objNetworkData').strBasenetwork)]"
          ]
        },
        "dhcpOptions": {
          "dnsServers": [
            "[concat(parameters('objNetworkData').strDnsServer.strDnsServer1)]",
            "[concat(parameters('objNetworkData').strDnsServer.strDnsServer2)]"
          ]
        },
        "subnets": [
          {
            "name": "[concat('snet-',parameters('objBaseData').strAppName,parameters('objBaseData').strAppEnv,parameters('objNetworkData').strSubnet1.strSubnetTier)]",
            "properties": {
              "addressPrefix": "parameters('objNetworkData').objSubnet.objSubnet1.strSubnetPrefix)"
            }
          }
        ]
      }
    }
  ]




}