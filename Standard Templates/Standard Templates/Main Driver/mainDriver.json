{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
  },
  "variables": {

    /*  
    +---------------------------------------------------------------------------------------------------------+
    |  Complex object for all uri path strings needed to link templates.                                      |
    +---------------------------------------------------------------------------------------------------------+*/
    "objUriPath": {
      /* "strTemplateBaseUri": "https://raw.githubusercontent.com/gstark77/PwCArm/master/Standard%20Templates/Standard%20Templates/Main%20Driver/",*/
      "strTemplateBaseUri": "https://github.com/gstark77/PwCArm/tree/master/Standard%20Templates/Standard%20Templates/Main%20Driver/",
      "strAppComponentFolderPath": "App%20Components/",
      "strAppComponentNetworkFileName": "appComponents.Network.json",
      "objBuildingBlocks": {
        "strBuildingBlockFolderPath": "Building%20Blocks/",
        "strAddVnetandOneSnetFileName": "addVnetandOneSnet.json"
      }
    },


    /* https://raw.githubusercontent.com/gstark77/PwCArm/master/Standard Templates/Standard Templates/Main Driver/Building Blocks/addVentandOneSnet.json  */
    /* https://github.com/gstark77/PwCArm/blob/master/Standard%20Templates/Standard%20Templates/Main%20Driver/App%20Components/appComponents.Network.json */
    /* https://github.com/gstark77/PwCArm/blob/master/Standard%20Templates/Standard%20Templates/Main%20Driver/Building%20Blocks/addVnetandOneSnet.json */

    /*  
    +---------------------------------------------------------------------------------------------------------+
    |  Complex object for the base data needed to name resources.                                             |
    +---------------------------------------------------------------------------------------------------------+*/
    "objBaseData": {
      "strAppName": "dar",
      "strAppEnv": "dev"
    },


    /*  
    +---------------------------------------------------------------------------------------------------------+
    |  Complex object for the base data needed to name resources.                                             |
    +---------------------------------------------------------------------------------------------------------+
        "strVnetName" "strVnetPrefix",
        "strSnetName" "strSnetPrefix",
        "strNsgName" "TBD_URD",
        "objNsgRule" "strVnetName",
        "strRemoteVnetName" "strResourceId"
    +---------------------------------------------------------------------------------------------------------+*/
    "objNetworkData": {
      "strBaseNetwork": "10.100.8.0/21",
      /* 0 = false, 1 = true (use in copy downstream to see if 0 is a valid arguement) */
      "testTrueFalseCreateVnet": "0"
    }

  },
    "resources": [
     {
       "apiVersion": "2015-01-01",
       "name": "linkedTemplate",
       "type": "Microsoft.Resources/deployments",
       "properties": {
       "mode": "Incremental",
         "templateLink": {
           "uri": "[concat(variables('objUriPath').strTemplateBaseUri,variables('objUriPath').strAppComponentFolderPath,variables('objUriPath').strAppComponentNetworkFileName)]",
           "contentVersion": "1.0.0.0"
          },
          "parameters": {
           "objUriPath": { "value": "[variables('objUriPath')]" },
            "objBaseData": { "value": "[variables('objBaseData')]" },
            "objNetworkData": { "value": "[variables('objNetworkData')]" }
          }
        }
      }
    ]
}