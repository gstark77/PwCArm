{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {

      "objUriPath": { "type": "object" },
      "objBaseData": { "type": "object" },
      "objNetworkData": { "type": "object" }

    },
    "variables": {

      /* Calculate vnet and snets here */

    },

  "resources": [
    {
      "apiVersion": "2015-01-01",
      "name": "[concat(deployment().name,'-networkLinkedTemplate-network')]", /*  needs to be unique */
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('objUriPath').strTemplateBaseUri,parameters('objUriPath').objBuildingBlocks.strBuildingBlockFolderPath,parameters('objUriPath').objBuildingBlocks.strAddVnetandOneSnetFileName)]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "objBaseData": { "value": "[parameters('objBaseData')]" },
          "objNetworkData": { "value": "[parameters('objNetworkData')]" }
        }
      }
    },
    {
      "apiVersion": "2015-01-01",
      "name": "[concat(deployment().name,'-networkLinkedTemplate-subnet')]",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('objUriPath').strTemplateBaseUri,parameters('objUriPath').objBuildingBlocks.strBuildingBlockFolderPath,parameters('objUriPath').objBuildingBlocks.strAddSnettoVnetFileName)]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "objBaseData": { "value": "[parameters('objBaseData')]" },
          "objNetworkData": { "value": "[parameters('objNetworkData')]" }
        }
      }
    }
  ]
}