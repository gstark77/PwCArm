{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "intArrayLoc": {"type":"int"},
    "networkInfo": { "type": "object" },
    "vnetNetworkInfo": { "type": "object" },
    "snetNetworkInfo": { "type": "object" }
  },
  "variables": {
    "networkConfiguration": {
      "dnsServer": "[parameters('networkInfo').dnsServer]",
      "vnetEnvironment": "[parameters('networkInfo').enviromentArraySelections[parameters('intArrayLoc')]]",
      "vnetName": "[parameters('vnetNetworkInfo').vnetName]",
      "vnetPrefix": "[parameters('vnetNetworkInfo').vnetPrefix]"
    }
  },
  "resources": [
    {
      "name": "vnet",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2016-09-01",
      "snetLoop": { "count": "[length(parameters('networkInfo').tierArray)]" },
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          /*"uri": "https://raw.githubusercontent.com/gstark77/PwCArm/master/Standard%20Templates/Standard%20Templates/Network/FinalSnetLinked.json"*/
    /*parameters('networkInfo').environmentArray[parameters('networkInfo').intArrayLoc]*/
    "uri": "[concat('https://raw.githubusercontent.com/gstark77/PwCArm/master/Standard%20Templates/Standard%20Templates/Network/', 'FinalSnetLinked','.Yes.json')]",
    "contentVersion": "1.0.0.0"
  },
        "parameters": {
          "vnetNetworkInfo": {"value": "[parameters('vnetNetworkInfo')]"},
          "networkInfo": {"value": "[parameters('networkInfo')]"},
          "snetNetworkInfo": {"value": "[parameters('snetNetworkInfo')]"}
        }
      }

    }
  ],
    "outputs": {
    }
}