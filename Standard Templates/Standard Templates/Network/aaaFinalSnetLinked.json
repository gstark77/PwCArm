{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {

    "strVnetName": { "type": "string" }, /* edw */
    "strVnetPrefix": { "type": "string" }, /* 10.60.0.0 */
    "strSnetName": { "type": "string" }, /* web   |   app   |   dev   */
    "strSnetPrefix": { "type": "string" }, /* 10.60.0.0  | 10.60.1.0  |  10.60.2.0  */
    "strDnsServer": { "type": "string" } /* 192.168.0.1 */


  },
  "variables": {
  },
  "resources": [
    {
      "apiVersion": "2015-05-01-preview",
      "type": "Microsoft.Network/virtualNetworks",
      "name": "[concat('snet_fromVISUALSTUDIO', uniquestring(resourcegroup().id))]",
      "location": "[resourceGroup().location]",
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[parameters('strVnetPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[parameters('strSnetName')]",
            "properties": {
              "addressPrefix": "[parameters('strSnetPrefix')]"
            }
          }
        ]
      }
    }
  ],
  "outputs": {
  }
}
